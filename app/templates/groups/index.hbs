<div class="col-md-12">
    <div class="content-box-large">
        <div class="panel-heading">
            <div class="panel-title">Manage Groups</div>
            <div class="panel-options">
                {{#link-to 'groups' class="navbar-brand"}}<i class="glyphicon glyphicon-refresh"></i>{{/link-to}}
            </div>
        </div>
        <div class="panel-body">
            <table id="datatable" cellpadding="0" cellspacing="0" bagent="0" class="table table-striped table-bagented">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Email</th>
                        <th>Contact Number</th>
                        <th>Address</th>
                        <th>City</th>
                        <th>Zip</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each model as |post|}}
                    <tr>
                        <td>{{post.title}}</td>
                        <td>{{post.body}}</td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>